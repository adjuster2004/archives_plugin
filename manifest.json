{
  "manifest_version": 3,
  "name": "Archive Image Downloader",
  "version": "1.1.0",
  "description": "Плагин для автоматического скачивания картинок от @adjuster2004",
  "author": "adjuster2004",
  "homepage_url": "https://github.com/adjuster2004/archives_plugin",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Archive Downloader"
  },
  "permissions": [
    "downloads"
  ],
  "host_permissions": [
    "*://anro.ryazan.gov.ru/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://anro.ryazan.gov.ru/*"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
